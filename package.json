    "dayjs": "1.11.7",
    "dotenv": "16.0.3",
    "fastify": "4.14.1",
    "npm-run-all": "4.1.5",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@rocketseat/eslint-config": "1.2.0",
    "@types/bcryptjs": "2.4.2",
    "@types/node": "18.15.3",
    "@types/supertest": "2.0.12",
    "@vitest/coverage-c8": "0.29.7",
    "eslint": "8.36.0",
    "prisma": "4.11.0",
    "supertest": "6.3.3",
    "tsup": "6.7.0",
    "tsx": "3.12.5",
